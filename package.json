{
  "name": "local-weather",
  "version": "0.37.0",
  "description": "Minimalist local weather app powered by Dark Sky",
  "private": true,
  "main": "index.js",
  "scripts": {
    "build": "yarn clean && yarn webpack && yarn lambda:build",
    "clean": "rm -rf ./public > /dev/null 2>&1",
    "start": "yarn dev:serve",
    "test": "echo \"Error: no test specified\" && exit 1",
    "webpack": "webpack --config ./webpack.config.js --progress --colors",
    "webpack:serve": "webpack-serve ./webpack.config.dev.js --port 5500 --open",
    "dev:build": "yarn dev:clean && yarn dev:webpack && yarn lambda:build",
    "dev:clean": "rm -rf ./build > /dev/null 2>&1",
    "dev:serve": "concurrently --kill-others --raw 'yarn webpack:serve' 'yarn lambda:serve'",
    "dev:watch": "yarn dev:clean && yarn dev:webpack --watch",
    "dev:webpack": "webpack --config ./webpack.config.dev.js --progress --colors",
    "lambda:build": "netlify-lambda build src/functions",
    "lambda:serve": "netlify-lambda serve src/functions -c lambda.config.dev.js"
  },
  "repository": {
    "url": "https://github.com/mikesprague/local-weather"
  },
  "author": {
    "name": "Michael Sprague",
    "email": "mikesprague@gmail.com"
  },
  "license": "GNU GPLv3",
  "bugs": {
    "url": "https://github.com/mikesprague/local-weather/issues"
  },
  "homepage": "https://localweather.io",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@fortawesome/fontawesome-pro": "^5.5.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.8",
    "@fortawesome/free-brands-svg-icons": "^5.5.0",
    "@fortawesome/pro-light-svg-icons": "^5.5.0",
    "@fortawesome/pro-regular-svg-icons": "^5.5.0",
    "@fortawesome/pro-solid-svg-icons": "^5.5.0",
    "babel-loader": "^8.0.4",
    "bugsnag": "^2.4.3",
    "bulma": "^0.7.2",
    "compression-webpack-plugin": "^2.0.0",
    "concurrently": "^4.1.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "dotenv": "^6.1.0",
    "eslint": "^5.9.0",
    "eslint-plugin-prettier": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.5",
    "netlify-cli": "^2.2.1",
    "netlify-lambda": "^1.0.3",
    "node-sass": "^4.10.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prettier": "^1.15.2",
    "request": "^2.34.0",
    "request-promise": "^4.2.2",
    "sass-lint": "^1.12.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "sweetalert2": "^7.29.1",
    "tippy.js": "^3.2.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^4.26.0",
    "webpack-bugsnag-plugins": "^1.2.2",
    "webpack-cli": "^3.1.2",
    "webpack-serve": "^2.0.3"
  }
}
